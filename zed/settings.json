{
  "auto_install_extensions": {
    "emmet": true,
    "html": true,
    "tokyo-night": true,
    "toml": true,
    "vue": true
  },
  "buffer_font_family": "Dank Mono",
  "buffer_font_size": 15,
  "buffer_line_height": {
    "custom": 1.7
  },
  "collaboration_panel": {
    "button": false
  },
  "edit_predictions": {
    "mode": "eager"
  },
  "experimental.theme_overrides": {
    "element.active": "#1e202e",
    "element.hover": "#1e202e",
    "element.selected": "#1e202e",
    "ghost_element.active": "#1e202e",
    "ghost_element.hover": "#1e202e",
    "ghost_element.selected": "#1e202e",
    "search.match_background": "#1e202e",
    "tab.active_background": "#1e202e"
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "git_panel": {
    "default_width": 320
  },
  "indent_guides": {
    "enabled": false
  },
  "languages": {
    "Markdown": {
      "preferred_line_length": 100,
      "show_wrap_guides": false,
      "soft_wrap": "bounded"
    }
  },
  "notification_panel": {
    "button": false
  },
  "outline_panel": {
    "default_width": 320,
    "indent_guides": {
      "show": "never"
    }
  },
  "project_panel": {
    "default_width": 320,
    "indent_guides": {
      "show": "never"
    }
  },
  "show_edit_predictions": false,
  "tab_size": 2,
  "tabs": {
    "file_icons": true,
    "git_status": true,
    "show_close_button": "always",
    "show_diagnostics": "all"
  },
  "telemetry": {
    "metrics": false
  },
  "theme": {
    "dark": "Tokyo Night",
    "light": "Tokyo Night Light",
    "mode": "dark"
  },
  "ui_font_family": ".SystemUIFont",
  "ui_font_size": 15,
  "vim_mode": true,
  "wrap_guides": [80]
}
