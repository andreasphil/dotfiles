{
	"Insert section": {
		"prefix": "section",
		"body": [
			"/* -------------------------------------------------- *",
			" * $1 *",
			" * -------------------------------------------------- */"
		]
	},
	"Test suite": {
		"prefix": "describe",
		"body": ["describe(\"${TM_FILENAME_BASE/([^\\.]+).*$/$1/}\", () => {$2})"]
	},
	"Test (it)": {
		"prefix": "it",
		"body": ["it(\"$1\", ${2:async} () => {", "  $3", "});"]
	}
}
