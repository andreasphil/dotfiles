{
    "atomKeymap.promptV3Features": true,
    "editor.fontSize": 14,
    "editor.formatOnPaste": true,
    "editor.lineHeight": 21,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderIndentGuides": false,
    "editor.renderWhitespace": "none",
    "editor.rulers": [100],
    "editor.tabSize": 2,
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 120,
    "editor.wrappingIndent": "indent",
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "files.hotExit": "onExitAndWindowClose",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "markdown.extension.italic.indicator": "_",
    "materialTheme.accent": "Indigo",
    "materialTheme.autoApplyIcons": true,
    "rest-client.enableTelemetry": false,
    "ruby.codeCompletion": "rcodetools",
    "ruby.format": "rubocop",
    "ruby.intellisense": "rubyLocate",
    "telemetry.enableTelemetry": false,
    "window.newWindowDimensions": "maximized",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Ayu Mirage",
    "workbench.iconTheme": "ayu",
    "workbench.settings.editor": "ui",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.feedback.visible": false,
    "ruby.lint": {
        "rubocop": true
    },
    "[markdown]": {
        "editor.quickSuggestions": false,
        "editor.rulers": [],
        "editor.wordWrap": "bounded",
    },
    "workbench.colorCustomizations": {
        "[Material Theme Palenight]": {
            "activityBar.background": "#212532",
            "activityBarBadge.background": "#5C6BC0",
            "breadcrumb.activeSelectionForeground": "#5C6BC0",
            "editor.lineHighlightBackground": "#212532",
            "editorGroupHeader.tabsBorder": "#212532",
            "editorRuler.foreground": "#212532",
            "editorSuggestWidget.highlightForeground": "#5C6BC0",
            "editorWidget.border": "#5C6BC0",
            "editorWidget.resizeBorder": "#5C6BC0",
            "input.border": "#333747",
            "list.activeSelectionForeground": "#5C6BC0",
            "list.highlightForeground": "#5C6BC0",
            "list.inactiveSelectionForeground": "#5C6BC0",
            "menu.selectionForeground": "#5C6BC0",
            "menubar.selectionForeground": "#5C6BC0",
            "notificationLink.foreground": "#5C6BC0",
            "panel.border": "#212532",
            "panelTitle.activeBorder": "#5C6BC0",
            "pickerGroup.foreground": "#5C6BC0",
            "progressBar.background": "#5C6BC0",
            "scrollbarSlider.activeBackground": "#5C6BC050",
            "settings.headerForeground": "#5C6BC0",
            "settings.modifiedItemIndicator": "#5C6BC0",
            "sideBar.border": "#212532",
            "statusBar.background": "#5C6BC0",
            "statusBar.border": "#212532",
            "statusBar.debuggingBackground": "#FFCD67",
            "statusBar.debuggingForeground": "#212532",
            "statusBar.foreground": "#ffffff",
            "tab.activeBorder": "#5C6BC0",
            "textLink.foreground": "#5C6BC0",
            "titleBar.border": "#212532",
        },
        "[Ayu Mirage]": {
            "activityBar.background": "#1a202b",
            "editorGroupHeader.tabsBorder": "#1a202b",
            "editorRuler.foreground": "#353b47",
            "focusBorder": "#212733",
            "sideBar.border": "#1a202b",
            "statusBar.background": "#1a202b",
            "statusBar.border": "#1a202b",
            "titleBar.border": "#1a202b",
        }
    },
}
