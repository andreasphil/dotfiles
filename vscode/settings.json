{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.quickSuggestions": false,
    "editor.rulers": [],
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 80
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "angular.enable-experimental-ivy-prompt": false,
  "breadcrumbs.enabled": false,
  "css.lint.unknownAtRules": "ignore",
  "debug.showInStatusBar": "never",
  "debug.toolBarLocation": "docked",
  "diffEditor.renderSideBySide": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.fontFamily": "\"Dank Mono\", FiraCode-Retina, Menlo, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.fontWeight": 600,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderIndentGuides": false,
  "editor.renderWhitespace": "none",
  "editor.rulers": [80, 100],
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[Ayu Mirage Bordered]": {
      "textMateRules": [
        {
          "scope": [
            "storage",
            "keyword.control",
            "keyword.other",
            "variable.language",
            "entity.name.method.js",
            "text.html.basic entity.other.attribute-name.html",
            "text.html.basic entity.other.attribute-name",
            "tag.decorator.js entity.name.tag.js",
            "tag.decorator.js punctuation.definition.tag.js",
            "source.js constant.other.object.key.js string.unquoted.label.js"
          ],
          "settings": {
            "fontStyle": "italic"
          }
        }
      ]
    }
  },
  "editor.wordWrapColumn": 120,
  "editor.wrappingIndent": "indent",
  "emmet.includeLanguages": {
    "erb": "html",
    "vue-html": "html"
  },
  "emmet.preferences": {
    "profile.allowCompactBoolean": true
  },
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.liquid": "html"
  },
  "files.hotExit": "onExitAndWindowClose",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.confirmSync": false,
  "git.untrackedChanges": "separate",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.quoteStyle": "single",
  "javascript.validate.enable": false,
  "ruby.format": "rubocop",
  "ruby.lint": {
    "rubocop": {
      "useBundler": true
    }
  },
  "ruby.useBundler": true,
  "ruby.useLanguageServer": true,
  "tailwindCSS.emmetCompletions": true,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontFamily": "FiraCode-Retina, Menlo, monospace",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.quoteStyle": "single",
  "vetur.format.defaultFormatter.html": "prettier",
  "vetur.format.enable": false,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.newWindowDimensions": "inherit",
  "window.restoreWindows": "one",
  "workbench.colorCustomizations": {
    "[Ayu Light Bordered]": {
      "editorRuler.foreground": "#dee8f1",
      "focusBorder": "#ff9940",
      "input.border": "#dee8f1"
    },
    "[Ayu Mirage Bordered]": {
      "editorRuler.foreground": "#191e2a",
      "focusBorder": "#fdc354",
      "input.border": "#191e2a"
    }
  },
  "workbench.colorTheme": "Ayu Mirage Bordered",
  "workbench.iconTheme": "ayu",
  "workbench.settings.editor": "ui",
  "workbench.tree.renderIndentGuides": "none"
}
