{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.quickSuggestions": false,
    "editor.rulers": [],
    "editor.wordWrap": "bounded"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "atomKeymap.promptV3Features": true,
  "breadcrumbs.enabled": false,
  "css.lint.unknownAtRules": "ignore",
  "debug.showInStatusBar": "never",
  "debug.toolBarLocation": "docked",
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.renderSideBySide": true,
  "editor.fontFamily": "FiraCode-Retina, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.lineHeight": 24,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderIndentGuides": false,
  "editor.renderWhitespace": "none",
  "editor.rulers": [
    100
  ],
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrapColumn": 120,
  "editor.wrappingIndent": "indent",
  "emmet.includeLanguages": {
    "erb": "html"
  },
  "eslint.validate": [
    "javascript",
    "vue"
  ],
  "extensions.ignoreRecommendations": true,
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.associations": {
    "*.liquid": "html"
  },
  "files.hotExit": "onExitAndWindowClose",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.untrackedChanges": "separate",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.quoteStyle": "single",
  "javascript.validate.enable": false,
  "markdown.extension.italic.indicator": "_",
  "materialTheme.accent": "Indigo",
  "prettier.disableLanguages": [
    "javascript"
  ],
  "prettier.printWidth": 100,
  "prettier.singleQuote": true,
  "ruby.codeCompletion": "rcodetools",
  "ruby.format": "rubocop",
  "ruby.intellisense": "rubyLocate",
  "ruby.lint": {
    "rubocop": true
  },
  "tailwindCSS.emmetCompletions": true,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.quoteStyle": "single",
  "vetur.format.enable": false,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.newWindowDimensions": "maximized",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "[Ayu Mirage]": {
      "activityBar.activeBackground": "#252A36",
      "activityBar.background": "#191e2a",
      "activityBar.activeBorder": "#fdc354",
      "editorGroupHeader.tabsBorder": "#191e2a",
      "editorRuler.foreground": "#191e2a",
      "focusBorder": "#212733",
      "sideBar.border": "#191e2a",
      "statusBar.background": "#191e2a",
      "statusBar.border": "#191e2a",
      "titleBar.border": "#191e2a"
    },
    "[Material Theme Palenight]": {
      "activityBar.background": "#212532",
      "activityBar.border": "#191e2a",
      "activityBarBadge.background": "#5C6BC0",
      "breadcrumb.activeSelectionForeground": "#5C6BC0",
      "editor.findMatchBorder": "#5C6BC0",
      "editor.lineHighlightBackground": "#212532",
      "editorGroupHeader.tabsBorder": "#212532",
      "editorRuler.foreground": "#212532",
      "editorSuggestWidget.highlightForeground": "#5C6BC0",
      "editorWidget.border": "#5C6BC0",
      "editorWidget.resizeBorder": "#5C6BC0",
      "input.border": "#333747",
      "list.activeSelectionForeground": "#5C6BC0",
      "list.highlightForeground": "#5C6BC0",
      "list.inactiveSelectionForeground": "#5C6BC0",
      "menu.selectionForeground": "#5C6BC0",
      "menubar.selectionForeground": "#5C6BC0",
      "notificationLink.foreground": "#5C6BC0",
      "panel.border": "#212532",
      "panelTitle.activeBorder": "#5C6BC0",
      "pickerGroup.foreground": "#5C6BC0",
      "progressBar.background": "#5C6BC0",
      "scrollbarSlider.activeBackground": "#5C6BC050",
      "selection.background": "#5C6BC040",
      "settings.headerForeground": "#5C6BC0",
      "settings.modifiedItemIndicator": "#5C6BC0",
      "sideBar.border": "#212532",
      "statusBar.background": "#5C6BC0",
      "statusBar.border": "#212532",
      "statusBar.debuggingBackground": "#FFCD67",
      "statusBar.debuggingForeground": "#212532",
      "statusBar.foreground": "#ffffff",
      "tab.activeBorder": "#5C6BC0",
      "textLink.foreground": "#5C6BC0",
      "titleBar.border": "#212532"
    }
  },
  "workbench.colorTheme": "Ayu Mirage",
  "workbench.iconTheme": "ayu",
  "workbench.settings.editor": "ui",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tree.renderIndentGuides": "none"
}
