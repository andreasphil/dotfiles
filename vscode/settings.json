{
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "[markdown]": {
    "editor.wordWrap": "bounded",
    "editor.wrappingIndent": "same"
  },
  "[yaml]": {
    "editor.guides.indentation": true
  },
  "breadcrumbs.enabled": false,
  "chat.commandCenter.enabled": false,
  "css.lint.unknownAtRules": "ignore",
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.accessibilitySupport": "off",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "\"Dank Mono\", FiraCode-Retina, Menlo, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.guides.indentation": false,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.inlineSuggest.enabled": true,
  "editor.lineHeight": 1.7,
  "editor.minimap.enabled": false,
  "editor.rulers": [
    80
  ],
  "editor.stickyScroll.enabled": false,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "editor.wordWrapColumn": 100,
  "editor.wrappingIndent": "indent",
  "emmet.preferences": {
    "profile.allowCompactBoolean": true
  },
  "extensions.ignoreRecommendations": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.openRepositoryInParentFolders": "always",
  "git.timeline.showUncommitted": true,
  "github.copilot.enable": {
    "*": false,
    "markdown": false,
    "plaintext": false,
    "scminput": false
  },
  "go.toolsManagement.autoUpdate": true,
  "gradle.nestedProjects": true,
  "inlineChat.lineNaturalLanguageHint": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.lineHeight": 1.4,
  "terminal.integrated.showExitAlert": false,
  "vim.foldfix": true,
  "vim.useSystemClipboard": true,
  "window.dialogStyle": "custom",
  "workbench.colorCustomizations": {
    "[Tokyo Night]": {
      "commandCenter.background": "#1a1b26",
      "editorGroup.border": "#16161e",
      "editorGroup.dropBackground": "#24283b75",
      "editorGroupHeader.border": "#1a1b26",
      "editorGroupHeader.tabsBackground": "#1a1b26",
      "editorOverviewRuler.border": "#1a1b26",
      "focusBorder": "#7aa2f7",
      "input.border": "#323343",
      "list.hoverBackground": "#00000015",
      "panel.border": "#16161e",
      "quickInput.background": "#1a1b26",
      "sideBar.border": "#16161e",
      "sideBarSectionHeader.border": "#16161e",
      "sideBarSectionHeader.foreground": "#9aa5ce90",
      "statusBar.border": "#16161e",
      "tab.activeBackground": "#1a1b26",
      "tab.activeBorder": "#1a1b26",
      "tab.activeBorderTop": "#7aa2f7",
      "tab.border": "#1a1b26",
      "tab.inactiveBackground": "#1a1b26",
      "tab.unfocusedActiveBorder": "#1a1b26",
      "tab.unfocusedActiveBorderTop": "#7aa2f7",
      "titleBar.border": "#16161e",
      "widget.shadow": "#00000025"
    }
  },
  "workbench.colorTheme": "Tokyo Night",
  "workbench.commandPalette.experimental.enableNaturalLanguageSearch": false,
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.editor.empty.hint": "hidden",
  "workbench.iconTheme": "eyecons",
  "workbench.layoutControl.enabled": false,
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "none",
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.renderIndentGuides": "none"
}
