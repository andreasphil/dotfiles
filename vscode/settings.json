{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.quickSuggestions": false,
    "editor.rulers": [],
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 80
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": false,
  "css.lint.unknownAtRules": "ignore",
  "debug.console.fontSize": 14,
  "debug.showInStatusBar": "never",
  "debug.toolBarLocation": "docked",
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.renderSideBySide": true,
  "editor.fontFamily": "'JetBrains Mono', FiraCode-Retina, Menlo, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.lineHeight": 24,
  "editor.minimap.enabled": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderIndentGuides": false,
  "editor.renderWhitespace": "none",
  "editor.rulers": [
    100
  ],
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrapColumn": 120,
  "editor.wrappingIndent": "indent",
  "emmet.includeLanguages": {
    "erb": "html"
  },
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.associations": {
    "*.liquid": "html"
  },
  "files.hotExit": "onExitAndWindowClose",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.confirmSync": false,
  "git.untrackedChanges": "separate",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.quoteStyle": "single",
  "javascript.validate.enable": false,
  "markdown.extension.italic.indicator": "_",
  "ruby.codeCompletion": "rcodetools",
  "ruby.format": "rubocop",
  "ruby.intellisense": "rubyLocate",
  "ruby.lint": {
    "rubocop": true
  },
  "tailwindCSS.emmetCompletions": true,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.quoteStyle": "single",
  "vetur.format.enable": false,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.newWindowDimensions": "inherit",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "[Ayu Light]": {
      "activityBar.activeBorder": "#ff9940",
      "activityBar.background": "#f0f1f2",
      "activityBar.border": "#dee8f1",
      "editorGroup.border": "#dee8f1",
      "editorGroupHeader.tabsBorder": "#dee8f1",
      "editorOverviewRuler.border": "#fafafa",
      "editorRuler.foreground": "#dee8f1",
      "focusBorder": "#ff9940",
      "input.border": "#dee8f1",
      "panel.border": "#dee8f1",
      "quickInput.background": "#fafafa",
      "sideBar.background": "#fafafa",
      "sideBar.border": "#dee8f1",
      "sideBarSectionHeader.background": "#fafafa",
      "statusBar.background": "#f0f1f2",
      "statusBar.border": "#dee8f1",
      "titleBar.border": "#dee8f1"
    },
    "[Ayu Mirage]": {
      "activityBar.activeBorder": "#fdc354",
      "activityBar.background": "#191e2a",
      "activityBar.border": "#141822",
      "editorGroup.border": "#141822",
      "editorGroupHeader.tabsBorder": "#141822",
      "editorOverviewRuler.border": "#1f2430",
      "editorRuler.foreground": "#141822",
      "focusBorder": "#fdc354",
      "input.border": "#141822",
      "panel.border": "#141822",
      "quickInput.background": "#1f2430",
      "sideBar.background": "#1f2430",
      "sideBar.border": "#141822",
      "sideBarSectionHeader.background": "#1f2430",
      "statusBar.background": "#191e2a",
      "statusBar.border": "#141822",
      "titleBar.border": "#141822"
    }
  },
  "workbench.colorTheme": "Ayu Mirage",
  "workbench.iconTheme": "ayu",
  "workbench.settings.editor": "ui",
  "workbench.tree.renderIndentGuides": "none"
}
