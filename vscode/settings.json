{
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[markdown]": {
    "editor.quickSuggestions": false,
    "editor.wordWrap": "bounded",
    "editor.wrappingIndent": "same"
  },
  "angular.enable-experimental-ivy-prompt": false,
  "breadcrumbs.enabled": false,
  "css.lint.unknownAtRules": "ignore",
  "editor.accessibilitySupport": "off",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "\"Dank Mono\", FiraCode-Retina, Menlo, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.guides.indentation": false,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 100,
  "editor.wrappingIndent": "indent",
  "emmet.includeLanguages": {
    "vue": "html"
  },
  "emmet.preferences": {
    "profile.allowCompactBoolean": true
  },
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.txt": "markdown"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "go.toolsManagement.autoUpdate": true,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.showExitAlert": false,
  "vscode-neovim.neovimExecutablePaths.darwin": "nvim",
  "window.autoDetectColorScheme": true,
  "workbench.colorTheme": "Ayu Mirage Bordered",
  "workbench.iconTheme": "ayu",
  "workbench.preferredDarkColorTheme": "Ayu Mirage Bordered",
  "workbench.preferredLightColorTheme": "Ayu Light Bordered",
  "workbench.tree.renderIndentGuides": "none"
}
